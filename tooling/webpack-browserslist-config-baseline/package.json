{
  "name": "webpack-browserslist-baseline-config",
  "version": "1.0.0",
  "description": "Shows you how to use browserslist-baseline-config with webpack",
  "main": "index.js",
  "scripts": {
    "clean": "npx rimraf dist",
    "build:dev": "npm run lint && npm run clean && NODE_ENV=development npx webpack --progress",
    "build": "npm run lint && npm run clean && NODE_ENV=production npx webpack --progress",
    "start": "npm run lint && npm run build && node http.js",
    "lint": "npx eslint ."
  },
  "keywords": [
    "webpack",
    "baseline",
    "browserslist",
    "browserslist-baseline-config"
  ],
  "author": "Jeremy L. Wagner <jlwagner@google.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@eslint/css": "^0.9.0",
    "@html-eslint/eslint-plugin": "^0.42.0",
    "@html-eslint/parser": "^0.42.0",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "browserslist-config-baseline": "^0.4.0",
    "core-js": "^3.43.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "cssnano": "^7.0.7",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "express": "^5.1.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "rimraf": "^6.0.1",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "browserslist": "extends browserslist-config-baseline"
}
